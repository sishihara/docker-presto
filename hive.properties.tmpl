connector.name=hive-hadoop2
hive.metastore.uri=thrift://hive-metastore:9083
hive.s3.connect-timeout=2m
hive.s3.max-backoff-time=10m
hive.s3.max-error-retries=50
hive.metastore-refresh-interval=1m
hive.s3.max-connections=500
hive.s3.max-client-retries=50
hive.s3.socket-timeout=2m
hive.metastore-cache-ttl=20m
hive.s3.staging-directory=/tmp/
hive.s3.use-instance-credentials=false
hive.s3.aws-access-key=${ACCESS_KEY_ID}
hive.s3.aws-secret-key=${SECRET_ACCESS_KEY}
hive.s3.endpoint=${ENDPOINT}
hive.s3.ssl.enabled=${ENABLE_SSL}
hive.s3.signer-type=S3SignerType